<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">

        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Hospital Patients</field>
            <field name="code">patient.patient</field>
            <field name="prefix">PAT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="seq_hospital_appointment" model="ir.sequence">
            <field name="name">Hospital Appointment</field>
            <field name="code">appointment.appointment</field>
            <field name="prefix">APP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>